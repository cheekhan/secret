{"code":"import { useYmdhInfo } from \"./dateTime\";\nimport { EarthlyBranch, default as branches } from \"./earthlyBranch\";\nimport { HeavenlyStem, default as stems } from \"./heavenlyStem\";\nimport { guiRenList } from \"./naturalOrgans\";\nimport { root, findNext, useJiaziNode } from \"./stemBranch\";\nimport {} from \"./yinyangWuxing\";\nconst useGuirenList = () => { return [...guiRenList]; }; // 获取贵人的拷贝\nexport { useYmdhInfo, // 获取四柱信息的函数\nEarthlyBranch, branches, // 地支类，和地支列表\nHeavenlyStem, stems, // 天干类，和天干列表\nuseGuirenList, // 贵人列表\nroot, findNext, useJiaziNode, // 甲子计算的方法\n };\n/**\n * 获取天盘布局\n * @param t\n * @param d\n * @returns\n */\nexport function useTianPie(t, d) {\n    const tianList = new Array(12);\n    let readListCount = 0; // 读取列表的次数\n    let tianBranchId = t.id - 1; // 从1开始\n    let diBranchId = d.id - 1;\n    while (readListCount < 12) {\n        tianList[diBranchId] = branches[tianBranchId];\n        readListCount += 1;\n        tianBranchId += 1;\n        if (tianBranchId >= 12)\n            tianBranchId = 0;\n        diBranchId += 1;\n        if (diBranchId >= 12)\n            diBranchId = 0;\n    }\n    return tianList;\n}\n/**\n * 获取贵人列表\n * stem:日干\n * time:时间，卯到酉，取昼\n * tian：天盘列表\n 甲戊庚牛羊\n 乙己鼠猴乡\n 丙丁猪鸡位\n 壬癸蛇兔藏\n 六辛在马虎\n */\nexport function useGuirenPie(stem, time, tian) {\n    let guirenRoot = branches[0];\n    if (stem.value === '甲' || stem.value === '戊' || stem.value === '庚') {\n        if (time.id >= 4 && time.id < 10) {\n            guirenRoot = branches[1];\n        }\n        else {\n            guirenRoot = branches[7];\n        }\n    }\n    else if (stem.value === '乙' || stem.value === '己') {\n        if (time.id >= 4 && time.id < 10) {\n            guirenRoot = branches[0];\n        }\n        else {\n            guirenRoot = branches[8];\n        }\n    }\n    else if (stem.value === '丙' || stem.value === '丁') {\n        if (time.id >= 4 && time.id < 10) {\n            guirenRoot = branches[11];\n        }\n        else {\n            guirenRoot = branches[9];\n        }\n    }\n    else if (stem.value === '壬' || stem.value === '癸') {\n        if (time.id >= 4 && time.id < 10) {\n            guirenRoot = branches[5];\n        }\n        else {\n            guirenRoot = branches[3];\n        }\n    }\n    else { // 六辛日\n        if (time.id >= 4 && time.id < 10) {\n            guirenRoot = branches[6];\n        }\n        else {\n            guirenRoot = branches[2];\n        }\n    }\n    let index = 0;\n    tian.some((e, i) => {\n        if (e.id === guirenRoot.id) {\n            index = i;\n            return true;\n        }\n    });\n    const arr = new Array(12); // 贵人列表   \n    if (index >= 5 && index < 11) { // 地户，到天门之间，逆向排\n        const gui = useGuirenList();\n        while (gui.length) {\n            if (index < 0) {\n                index = 11;\n            }\n            const n = UTS.arrayShift(gui);\n            arr[index] = n;\n            index -= 1;\n        }\n    }\n    else {\n        const gui = useGuirenList();\n        while (gui.length) {\n            if (index > 11) {\n                index = 0;\n            }\n            const n = UTS.arrayShift(gui);\n            arr[index] = n;\n            index += 1;\n        }\n    }\n    return arr;\n}\n//# sourceMappingURL=/Users/cheekhan/Documents/GitHub/secret/app/pages/equation/setup/index.uts.js.map","references":[],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"index.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"index.uts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,WAAW,EAAE,mBAAsB;AAC5C,OAAO,EAAE,aAAa,EAAE,OAAO,IAAI,QAAQ,EAAE,wBAA2B;AACxE,OAAO,EAAE,YAAY,EAAE,OAAO,IAAI,KAAK,EAAE,uBAA0B;AACnE,OAAO,EAAE,UAAU,EAAe,wBAA2B;AAC7D,OAAO,EAAE,IAAI,EAAE,QAAQ,EAAE,YAAY,EAAE,qBAAwB;AAC/D,OAAO,EAA2C,wBAA2B;AAC7E,MAAM,aAAa,GAAG,QAAM,OAAA,CAAC,GAAG,UAAU,CAAC,EAAf,CAAe,CAAA,CAAC,UAAU;AACtD,OAAO,EACN,WAAW,EAAE,YAAY;AACzB,aAAa,EAAE,QAAQ,EAAE,YAAY;AACrC,YAAY,EAAE,KAAK,EAAE,YAAY;AACjC,aAAa,EAAc,OAAO;AAClC,IAAI,EAAE,QAAQ,EAAE,YAAY,E